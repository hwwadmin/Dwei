<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dwei.domain.mapper.DictMapper">

    <resultMap id="BaseResultMap" type="com.dwei.domain.entity.DictEntity">
            <id property="id" column="id"/>
            <result property="name" column="name"/>
            <result property="code" column="code"/>
            <result property="remark" column="remark"/>
            <result property="enable" column="enable" javaType="com.dwei.common.enums.YesNoEnum"/>
            <result property="createTime" column="create_time"/>
            <result property="updateTime" column="update_time"/>
            <result property="deleteTime" column="delete_time"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,code,
        remark,enable,create_time,
        update_time,delete_time
    </sql>

</mapper>
