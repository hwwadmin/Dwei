        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dwei.domain.mapper.PermissionMapper">

    <resultMap id="BaseResultMap" type="com.dwei.domain.entity.PermissionEntity">
